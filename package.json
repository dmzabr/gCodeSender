{
  "name": "gcode-sender",
  "productName": "gcode-sender",
  "version": "1.0.0",
  "description": "gCode sender",
  "main": "src/main.js",
  "scripts": {
    "dev": "npx electronmon .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-css": "npx tailwindcss -i ./src/renderer/input.css -o ./src/renderer/css/style.css",
    "css-watch": "npx tailwindcss -i ./src/renderer/input.css -o ./src/renderer/css/style.css --watch"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
